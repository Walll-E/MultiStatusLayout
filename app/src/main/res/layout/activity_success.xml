<?xml version="1.0" encoding="utf-8"?>
<com.wall_e.multiStatusLayout.MultiStatusLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:loadingLayout="@layout/common_loading"
    app:targetViewId="@+id/rl_title"
    android:id="@+id/statusLayout"
    tools:context="com.example.wall_e.multistatuslayout.SuccessActivity">
    <include layout="@layout/title"/>
    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_below="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.wall_e.multiStatusLayout.MultiStatusLayout>
